#: model:mail.template,body_html:odoo_orchest.email_template_instance_deployed
msgid "\n"
"<div class=\"or-email\" style=\"font-family: sans-serif; margin: 0; padding: 10px;\">\n"
"  <h1 class=\"or-orchest\" style=\"padding: 0; margin: 10px 0;\"><span class=\"or-color-vx or-text-big\" style=\"margin: 0; padding: 0; color: #A41D35; font-size: 1.5em;\">O</span>rchest</h1>\n"
"  <div class=\"or-content\" style=\"margin: 0; padding: 10px; background-color: #ECECEC; border-radius: 5px;\">\n"
"<p class=\"or-alert-success or-text-big\" style=\"font-size: 1.5em; margin: 10px 0; padding: 2px; border-radius: 5px; color: black; background-color: #99FF55;\">Your instance has been deployed!</p>\n"
"<p class=\"or-menu\" style=\"padding: 0; margin: 10px 0;\">\n"
"  % if ctx.direct_url:\n"
"  <a href=\"${ctx.direct_url}\" style=\"margin: 0; background-color: #A41D35; border-radius: 5px; padding: 5px; color: white; margin-right: 5px;\">Access instance</a>\n"
"  % endif\n"
"  % if ctx.nginx_url:\n"
"  <a href=\"${ctx.nginx_url}\" style=\"margin: 0; background-color: #A41D35; border-radius: 5px; padding: 5px; color: white; margin-right: 5px;\">Access Nginx</a>\n"
"  % endif\n"
"</p>\n"
"% if ctx.admin and ctx.adm_pass:\n"
"<table class=\"or-table\" style=\"margin: 0; padding: 0;\">\n"
"  <tbody style=\"margin: 0; padding: 0;\">\n"
"    <tr style=\"margin: 0; padding: 0;\">\n"
"      <td style=\"margin: 0; padding: 5px; padding-left: 0;\">\n"
"        <b style=\"margin: 0; padding: 0;\">Admin password:</b>\n"
"      </td>\n"
"      <td style=\"margin: 0; padding: 5px; padding-right: 0;\">\n"
"        ${ctx.admin}\n"
"      </td>\n"
"    </tr>\n"
"    <tr style=\"margin: 0; padding: 0;\">\n"
"      <td style=\"margin: 0; padding: 5px; padding-left: 0;\">\n"
"        <b style=\"margin: 0; padding: 0;\">Super admin password:</b>\n"
"      </td>\n"
"      <td style=\"margin: 0; padding: 5px; padding-right: 0;\">\n"
"        ${ctx.adm_pass}\n"
"      </td>\n"
"    </tr>\n"
"  </tbody>\n"
"</table>\n"
"% endif\n"
"% if ctx.db_name:\n"
"<table class=\"or-table\" style=\"margin: 0; padding: 0;\">\n"
"  <tbody style=\"margin: 0; padding: 0;\">\n"
"    <tr style=\"margin: 0; padding: 0;\">\n"
"      <td style=\"margin: 0; padding: 5px; padding-left: 0;\">\n"
"        <b style=\"margin: 0; padding: 0;\">Database name:</b>\n"
"      </td>\n"
"      <td style=\"margin: 0; padding: 5px; padding-right: 0;\">\n"
"        ${ctx.db_name}\n"
"      </td>\n"
"    </tr>\n"
"  </tbody>\n"
"</table>\n"
"% endif\n"
"% if ctx.backup:\n"
"<table class=\"or-table\" style=\"margin: 0; padding: 0;\">\n"
"  <tbody style=\"margin: 0; padding: 0;\">\n"
"    <tr style=\"margin: 0; padding: 0;\">\n"
"      <td style=\"margin: 0; padding: 5px; padding-left: 0;\">\n"
"        <b style=\"margin: 0; padding: 0;\">Backup used:</b>\n"
"      </td>\n"
"      <td style=\"margin: 0; padding: 5px; padding-right: 0;\">\n"
"        ${ctx.backup}\n"
"      </td>\n"
"    </tr>\n"
"  </tbody>\n"
"</table>\n"
"% endif\n"
"<p class=\"or-menu\" style=\"padding: 0; margin: 10px 0;\">\n"
"  % if ctx.deploy_url:\n"
"  <a href=\"${ctx.deploy_url}\" style=\"margin: 0; background-color: #A41D35; border-radius: 5px; padding: 5px; color: white; margin-right: 5px;\">Verify the deploy</a>\n"
"  % endif\n"
"  % if ctx.history_url:\n"
"  <a href=\"${ctx.history_url}\" style=\"margin: 0; background-color: #A41D35; border-radius: 5px; padding: 5px; color: white; margin-right: 5px;\">Build history</a>\n"
"  % endif\n"
"</p>\n"
"% if ctx.ssh_access:\n"
"<div class=\"or-separator\" style=\"padding: 0; height: 2px; background-color: #888888; margin: 20px 0 10px 0; border-radius: 5px;\"></div>\n"
"<p class=\"or-text or-text-big\" style=\"margin: 0; padding: 0; font-size: 1.5em; margin-bottom: 5px;\">\n"
"  <b style=\"margin: 0; padding: 0;\">SSH Access:</b>\n"
"</p>\n"
"<ul class=\"or-list\" style=\"padding: 0; margin: 10px 0; margin-left: 25px;\">\n"
"    <li style=\"margin: 0; padding: 0;\">ssh -p ${ctx.ssh_access.get('port')} odoo@${ctx.ssh_access.get('domain')} </li>\n"
"    <li style=\"margin: 0; padding: 0;\">ssh -p ${ctx.ssh_access.get('port')} root@${ctx.ssh_access.get('domain')} </li>\n"
"</ul>\n"
"<p class=\"or-text\" style=\"margin: 0; padding: 0; margin-bottom: 5px;\">\n"
"  To verify the odoo logs see: <i style=\"margin: 0; padding: 0;\">/var/log/supervisor/odoo_stdout.log</i>\n"
"</p>\n"
"<p class=\"or-text\" style=\"margin: 0; padding: 0; margin-bottom: 5px;\">\n"
"  To verify the postgres logs see:\n"
"</p>\n"
"<ul class=\"or-list\" style=\"padding: 0; margin: 10px 0; margin-left: 25px;\">\n"
"  <li style=\"margin: 0; padding: 0;\"><i style=\"margin: 0; padding: 0;\">/var/log/supervisor/postgres_stdout.log</i></li>\n"
"  <li style=\"margin: 0; padding: 0;\"><i style=\"margin: 0; padding: 0;\">/var/log/supervisor/postgres_stderr.log</i></li>\n"
"</ul>\n"
"<p class=\"or-text\" style=\"margin: 0; padding: 0; margin-bottom: 5px;\">To manage the instance of odoo use the command supervisorctl:</p>\n"
"<ul class=\"or-list\" style=\"padding: 0; margin: 10px 0; margin-left: 25px;\">\n"
"  <li style=\"margin: 0; padding: 0;\">supervisorctl start odoo</li>\n"
"  <li style=\"margin: 0; padding: 0;\">supervisorctl stop odoo</li>\n"
"  <li style=\"margin: 0; padding: 0;\">supervisorctl status odoo</li>\n"
"  <li style=\"margin: 0; padding: 0;\">supervisorctl restart odoo</li>\n"
"</ul>\n"
"<div class=\"or-separator\" style=\"padding: 0; height: 2px; background-color: #888888; margin: 20px 0 10px 0; border-radius: 5px;\"></div>\n"
"<p class=\"or-text or-text-big\" style=\"margin: 0; padding: 0; font-size: 1.5em; margin-bottom: 5px;\">\n"
"  <b style=\"margin: 0; padding: 0;\">Warning!</b>\n"
"</p>\n"
"<p class=\"or-text\" style=\"margin: 0; padding: 0; margin-bottom: 5px;\">You must <b style=\"margin: 0; padding: 0;\">not</b> stop the supervisor process!</p>\n"
"% endif\n"
"<div class=\"or-separator\" style=\"padding: 0; height: 2px; background-color: #888888; margin: 20px 0 10px 0; border-radius: 5px;\"></div>\n"
"% if ctx.direct_url:\n"
"<p class=\"or-text or-text-small or-color-muted\" style=\"margin: 0; padding: 0; color: #888888; font-size: .75em; margin-bottom: 5px;\">\n"
"  Direct URL: ${ctx.direct_url}\n"
"</p>\n"
"% endif\n"
"% if ctx.nginx_url:\n"
"<p class=\"or-text or-text-small or-color-muted\" style=\"margin: 0; padding: 0; color: #888888; font-size: .75em; margin-bottom: 5px;\">\n"
"  Nginx URL: ${ctx.nginx_url}\n"
"</p>\n"
